{
	"name": "Updateable Bookmarks",
	"version": "0.9",
	"permissions": [
	  "bookmarks", "tabs", "storage"
	],
	"background": {
		"persistent": false,
		"scripts": [
			"background.js"
		]
	},

	"browser_action": {
		"default_title": "Updateable Bookmarks",
		"default_icon":  "icon.png",
		"default_popup": "popup/popup.html"
	},

	"options_ui": {
		"page": "options/options.html",
		"open_in_tab": false
	},
	"manifest_version": 2
}